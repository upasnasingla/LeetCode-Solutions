class Solution {
    public boolean isPerfectSquare(int num) {
        boolean ans=false;
        int n=num/2;
        for(int i=2;i<=n;i++){
             if((i*i)==num){
                 ans=true;
                 return ans;
             }
        }
        if(num==1){
            return true;
        }
        return ans;
    }
}
